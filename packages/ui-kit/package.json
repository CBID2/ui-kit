    "@types/testing-library__jest-dom": "^5.14.7",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^27.5.1",
    "msw": "^1.2.2",
    "react": "^18.2.0",
